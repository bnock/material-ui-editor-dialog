{
    "name": "material-ui-editor-dialog",
    "version": "0.1.0",
    "description": "React editor dialog built using Material UI",
    "main": "dist/material-ui-editor-dialog.js",
    "repository": "https://github.com/bnock/material-ui-editor-dialog.git",
    "author": "Bradford Nock <bradford.nock@gmail.com>",
