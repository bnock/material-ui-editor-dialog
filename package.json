        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-quill-2": "^2.0.2",
        "react-redux": "^7.0.3",
        "redux": "^4.0.1",
        "redux-thunk": "^2.3.0",
        "typeface-roboto": "^0.0.54"
    },
    "devDependencies": {
        "@babel/core": "^7.4.5",
        "@babel/plugin-proposal-class-properties": "^7.4.4",
